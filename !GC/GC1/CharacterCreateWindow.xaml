<Window x:Class="GC1.CharacterCreateWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GC1"
        mc:Ignorable="d"
        Title="Diablo" Width="1360" Height="768" WindowState="Normal" WindowStyle="None" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" ShowInTaskbar="True" Icon="\images\DiaboIcon.png">
    <Grid Background="Black">
        <Image Margin="573,224,573,224" Source="/images/assasin.png" Stretch="Fill" MouseDown="SelectAssasin_MouseDown"/>
        <Image Margin="882,222,264,225" Source="/images/sorcerer.png" Stretch="Fill" MouseDown="SelectSorcerer_MouseDown"/>
        <Image x:Name="SelectWarrior" Margin="266,222,880,224" Source="/images/warrior.png" Stretch="Fill" MouseDown="SelectWarrior_MouseDown"/>
        <Button x:Name="ReturnButton" Content="Return" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="ReturnButton_Click"/>
    </Grid>
</Window>
